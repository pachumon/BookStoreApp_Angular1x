    "concurrently": "^4.1.0",
    "jasmine-core": "^3.4.0",
    "json-server": "^0.15.0",
    "karma": "^6.3.16",
    "karma-babel-preprocessor": "^8.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-clear-screen-reporter": "^1.0.0",
