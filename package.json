{
  "name": "webapptemplate",
  "version": "1.0.0",
  "description": "just a webapp template with bootstrap and jquery setup",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start-server\" \"npm run build\" \"npm run start-api\"",
    "start-server": "npx live-server --browser=chrome",
    "test": "npx karma start karma.conf.js",
    "build": "npx webpack --mode development -w",
    "start-api": "json-server --watch api/books.json"
  },
